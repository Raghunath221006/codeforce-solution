#include <bits/stdc++.h>
using namespace std;

int main() {
	string a;
	cin>>a;
	int l=0;
	int u=0;
	for(auto x:a){
	    if(x==toupper(x)){
	        u++;
	    }
	    else{
	        l++;
	    }
	}
	if(l>=u){
	    for(auto& x:a){
	        x=tolower(x);
	    
	    }
	}
	else{
	    for(auto& x:a){
	        x=toupper(x);
	    }
	}
	cout<<a;


}
